<div class="container mt-3">
    <div class="row">
        <div class="col-md8">
            <h2>Detail Contact</h2>
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                  <h5 class="card-title">Nama :   <%= contact.name %> </h5>
                  <h6 class="card-subtitle mb-2 text-muted">Nomer :<%= contact.phone %> </h6>
                  <p class="card-text">Email : <%= contact.email %> </p>
                  <a href="/contact" class="card-link">Back to Contact List</a>
                </div>
              </div>
        </div>
    </div>
</div>